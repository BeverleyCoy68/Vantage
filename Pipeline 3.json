						"type": "Expression"
					},
					"activities": [
						{
							"name": "Set variable1",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "varItem",
								"value": "@item().Identifier"
							}
						}
					]
				}
			}
		],
		"variables": {
			"varItem": {
				"type": "String"
			}
		},
		"annotations": []
	}
